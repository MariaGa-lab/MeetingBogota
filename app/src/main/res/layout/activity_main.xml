<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="367dp"
        android:layout_height="713dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="8dp"
        android:background="@color/teal_200"
        android:backgroundTint="@color/darkseagreen"
        android:scrollbarStyle="outsideInset"
        android:visibility="visible"
        app:flow_horizontalAlign="start"
        app:flow_verticalAlign="top"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck"
        tools:visibility="visible">

    </ScrollView>

    <TextView
        android:id="@+id/textView6"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="28dp"
        android:contentDescription="@string/descriptionBogota"
        android:justificationMode="none"
        android:text="@string/textViewInformacion"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="177dp"
        android:contentDescription="@string/descriptionBogota"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2"
        app:srcCompat="@drawable/bogota_1" />

    <Button
        android:id="@+id/button2"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="217dp"
        android:layout_marginEnd="200dp"
        android:text="@string/ButtonGoldMuseum"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

    <Button
        android:id="@+id/button3"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="217dp"
        android:layout_marginEnd="32dp"
        android:text="@string/ButtonBolivarPark"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

    <Button
        android:id="@+id/button4"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="267dp"
        android:layout_marginEnd="200dp"
        android:text="@string/ButtonWorldAdventure"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

    <Button
        android:id="@+id/button5"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="267dp"
        android:layout_marginEnd="32dp"
        android:text="@string/ButtonGardenBotanic"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="317dp"
        android:layout_marginEnd="200dp"
        android:text="@string/ButtonChorroQuevedo"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="@+id/scrollView2"
        app:layout_constraintTop_toTopOf="@+id/scrollView2"
        tools:visibility="visible" />

    <Button
        android:id="@+id/button7"
        android:layout_width="164dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="317dp"
        android:layout_marginEnd="32dp"
        android:text="@string/ButtonPlazaBolivar"
        app:layout_constraintEnd_toEndOf="@+id/scrollView2"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toTopOf="@+id/scrollView2" />

</androidx.constraintlayout.widget.ConstraintLayout>